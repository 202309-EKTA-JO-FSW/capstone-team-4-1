{
  "name": "Capstone-Server",
  "version": "1.0.0",
  "description": "Express server for capstone",
  "main": "index.js",
  "scripts": {
    "start:app": "NODE_ENV=development nodemon server/index.js",
    "postinstallOld": "CONT=$(docker ps -aqf name=node-app) && [ $CONT ] && docker exec -w /backend-app $CONT npm i || true",
    "start": "docker-compose up",
    "build": "docker-compose up --build",
    "prune": "docker-compose down -v",
    "stop": "docker-compose down",
    "test:app": "NODE_ENV=test mocha --exit --require mocha-steps ./server/tests",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^4.1.2",
    "mongoose": "^6.0.7",
    "multer": "^1.4.5-lts.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "jest": "^29.7.0",
    "mocha": "^9.1.1",
    "mocha-steps": "^1.3.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4"
  }
}
